<!DOCTYPE html>
<html>
<head>
    <title>Query Result</title>
</head>
<body>
    <h2>Query Result</h2>
    <% if (results.length > 0) { %>
    <table border="1">
        <thead>
            <tr>
                <% Object.keys(results[0]).forEach(function(key) { %>
                    <th><%= key %></th>
                <% }); %>
            </tr>
        </thead>
        <tbody>
            <% results.forEach(row => { %>
            <tr>
                <% Object.values(row).forEach(function(value) { %>
                    <td><%= value %></td>
                <% }); %>
            </tr>
            <% }); %>
        </tbody>
    </table>
    <% } else { %>
        <p>No results found.</p>
    <% } %>
</body>
</html>
